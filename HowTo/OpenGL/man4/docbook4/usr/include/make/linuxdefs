/*
 * defines to make gcc's -ansi more-or-less equivalent to SGI's -xansi
 */

#if defined(__i386__)
#define i386 1
#endif

#if defined(__linux__)
#define linux 1
#endif

#if defined(__unix__)
#define unix 1
#endif

#if !defined(_BSD_SOURCE)
#define _BSD_SOURCE 1
#endif
